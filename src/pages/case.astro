---
import Layout from '@/layouts/Layout.astro'
import { Image } from 'astro:assets'
import logo from '../../public/AboutLogo.627a361.png'
import CaseTabs from '@/components/CaseTabs.vue'
---

<Layout title="上海疆通科技有限公司" active="4">
  <main>
    <div class="w-full">
      <div class="w-full relative">
        <Image src={logo} alt="背景图片" />
        <div class="absolute top-0 pt-[100px] px-[20%]">
          <h5 class="text-white text-[50px] mb-[20px] mt-0">成功案例</h5>
          <div class="bg-white h-[4px] w-[34px]"></div>
        </div>
      </div>
      <CaseTabs client:visible />
    </div>
  </main>
</Layout>

